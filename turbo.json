{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["prisma:generate","^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "env": ["OPENAI_API_KEY"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["OPENAI_API_KEY", "DATABASE_URL"]
    },
    "start": {
      "cache": false,
      "persistent": true,
      "env": ["OPENAI_API_KEY", "DATABASE_URL"]
    },
    "clean": {
      "cache": false
    },
    "prisma:generate": {
      "cache": false
    }
  }
}
